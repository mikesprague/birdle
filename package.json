{
  "name": "birdle",
  "version": "1.9.11",
  "description": "birdle",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run webpack -- --mode production",
    "clean": "rm -rf ./dist",
    "dev": "concurrently 'npm run watch' 'npm run serve' --names 'ðŸ“¦,ðŸ–¥' --prefix '[{name}] ' --kill-others",
    "start": "npm run dev",
    "serve": "serve -S ./dist -p 1234",
    "get-ver": "cross-env APP_VERSION=$(echo $npm_package_version)",
    "play": "npm show . version",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "cross-env NODE_ENV=development npm run webpack -- watch --mode development",
    "webpack": "webpack --color"
  },
  "keywords": [],
  "repository": {
    "url": "https://github.com/mikesprague/birdle"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "engines": {
    "node": ">= 16.x",
    "npm": ">= 8.x"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikesprague/birdle/issues"
  },
  "homepage": "https://birdle.app",
  "dependencies": {
    "@sweetalert2/theme-dark": "5.0.11",
    "clipboard": "2.0.11",
    "common-tags": "1.8.2",
    "dayjs": "1.11.3",
    "keyboardjs": "2.6.4",
    "register-service-worker": "1.7.2",
    "sweetalert2": "11.4.22"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/eslint-parser": "7.18.2",
    "@babel/eslint-plugin": "7.17.7",
    "@babel/plugin-transform-runtime": "7.18.6",
    "@babel/preset-env": "7.18.6",
    "@babel/runtime": "7.18.6",
    "@fullhuman/postcss-purgecss": "4.1.3",
    "autoprefixer": "10.4.7",
    "charts.css": "0.9.0",
    "compression-webpack-plugin": "10.0.0",
    "concurrently": "7.2.2",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "cssnano": "5.1.12",
    "esbuild-loader": "2.19.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-stylelint": "15.1.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.6.1",
    "postcss": "8.4.14",
    "postcss-cli": "10.0.0",
    "postcss-loader": "7.0.1",
    "prettier": "2.7.1",
    "prettier-eslint": "15.0.1",
    "sass": "1.53.0",
    "sass-loader": "13.0.2",
    "serve": "14.0.1",
    "style-loader": "3.3.1",
    "stylelint": "14.9.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard": "26.0.0",
    "stylelint-config-standard-scss": "4.0.0",
    "stylelint-prettier": "2.0.0",
    "tailwindcss": "3.1.6",
    "url-loader": "4.1.1",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "workbox-webpack-plugin": "6.5.3"
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": false,
          "helpers": true,
          "regenerator": true,
          "useESModules": true
        }
      ]
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": {
    "development": [
      "last 1 version"
    ],
    "production": [
      "> 1%",
      "last 2 versions",
      "not ie <= 10"
    ]
  },
  "eslintConfig": {
    "settings": {
      "jest": {
        "version": 26
      }
    },
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "prettier",
      "stylelint"
    ],
    "plugins": [
      "babel",
      "compat",
      "html",
      "import",
      "prettier"
    ],
    "parserOptions": {
      "parser": "babel-parser",
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true
      }
    },
    "rules": {},
    "ignorePatterns": [
      "dist/",
      ".history/",
      ".parcel-cache/",
      "node_modules/"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "tabWidth": 2
  }
}
