{
  "name": "birdle",
  "version": "1.6.9",
  "description": "birdle",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production yarn webpack --mode production",
    "clean": "rm -rf ./dist",
    "dev": "concurrently 'yarn watch' 'yarn serve' --names 'ðŸ“¦,ðŸ–¥' --prefix '[{name}] ' --kill-others",
    "start": "yarn dev",
    "serve": "serve -S ./dist -p 1234",
    "get-ver": "cross-env APP_VERSION=$(echo $npm_package_version)",
    "play": "npm show . version",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "cross-env NODE_ENV=development yarn webpack watch --mode development",
    "webpack": "webpack --color"
  },
  "keywords": [],
  "repository": "https://github.com/mikesprague/birdle",
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikesprague/birdle/issues"
  },
  "homepage": "https://birdle.app",
  "dependencies": {
    "@sweetalert2/theme-dark": "5.0.10",
    "clipboard": "2.0.10",
    "common-tags": "1.8.2",
    "dayjs": "1.11.0",
    "keyboardjs": "2.6.4",
    "register-service-worker": "1.7.2",
    "sweetalert2": "11.4.4"
  },
  "devDependencies": {
    "@babel/core": "7.17.7",
    "@babel/eslint-parser": "7.17.0",
    "@babel/eslint-plugin": "7.17.7",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/runtime": "7.17.7",
    "@fullhuman/postcss-purgecss": "4.1.3",
    "@typescript-eslint/eslint-plugin": "5.15.0",
    "@typescript-eslint/parser": "5.15.0",
    "autoprefixer": "10.4.2",
    "babel-loader": "8.2.3",
    "charts.css": "0.9.0",
    "compression-webpack-plugin": "9.2.0",
    "concurrently": "7.0.0",
    "copy-webpack-plugin": "10.2.4",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "3.4.1",
    "cssnano": "5.1.4",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-stylelint": "15.0.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.6.0",
    "postcss": "8.4.8",
    "postcss-cli": "9.1.0",
    "postcss-loader": "6.2.1",
    "prettier": "2.5.1",
    "prettier-eslint": "13.0.0",
    "stylelint-prettier": "2.0.0",
    "sass": "1.49.9",
    "sass-loader": "12.6.0",
    "serve": "13.0.2",
    "style-loader": "3.3.1",
    "stylelint": "14.5.3",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard": "25.0.0",
    "stylelint-config-standard-scss": "3.0.0",
    "tailwindcss": "3.0.23",
    "terser-webpack-plugin": "5.3.1",
    "url-loader": "4.1.1",
    "typescript": "4.6.2",
    "webpack": "5.70.0",
    "webpack-cli": "4.9.2",
    "workbox-webpack-plugin": "6.5.1"
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": false,
          "helpers": true,
          "regenerator": true,
          "useESModules": true
        }
      ]
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": {
    "development": [
      "last 1 version"
    ],
    "production": [
      "> 1%",
      "last 2 versions",
      "not ie <= 10"
    ]
  },
  "eslintConfig": {
    "settings": {
      "jest": {
        "version": 26
      }
    },
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "prettier",
      "stylelint"
    ],
    "plugins": [
      "babel",
      "compat",
      "html",
      "import",
      "prettier"
    ],
    "parserOptions": {
      "parser": "babel-parser",
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true
      }
    },
    "rules": {},
    "ignorePatterns": [
      "dist/",
      ".history/",
      ".parcel-cache/",
      "node_modules/"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "tabWidth": 2
  }
}
