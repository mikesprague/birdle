{
  "name": "birdle",
  "version": "1.0.0",
  "description": "birdle",
  "private": true,
  "scripts": {
    "build": "parcel build src/index.html",
    "dev": "workbox generateSW && parcel src/index.html",
    "postbuild": "workbox generateSW",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "repository": "https://github.com/mikesprague/birdle",
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikesprague/birdle/issues"
  },
  "homepage": "https://birdle.app",
  "dependencies": {
    "@sweetalert2/theme-dark": "5.0.8",
    "clipboard": "2.0.10",
    "common-tags": "1.8.2",
    "dayjs": "1.10.7",
    "keyboardjs": "2.6.4",
    "sweetalert2": "11.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.2",
    "@babel/eslint-parser": "7.17.0",
    "@babel/eslint-plugin": "7.16.5",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/runtime": "7.17.2",
    "@parcel/packager-raw-url": "2.3.0",
    "@parcel/service-worker": "2.2.1",
    "@parcel/transformer-sass": "2.2.1",
    "@parcel/transformer-webmanifest": "2.2.1",
    "@types/express": "4.17.13",
    "@types/node": "17.0.16",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "autoprefixer": "10.4.2",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "parcel": "2.2.1",
    "postcss": "8.4.6",
    "postcss-custom-properties": "12.1.4",
    "postcss-import": "14.0.2",
    "postcss-url": "10.1.3",
    "prettier": "2.5.1",
    "prettier-eslint": "13.0.0",
    "sass": "1.49.7",
    "sass-lint": "1.13.1",
    "tailwindcss": "3.0.19",
    "typescript": "4.5.5",
    "workbox-cli": "^6.4.2"
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": false,
          "helpers": true,
          "regenerator": true,
          "useESModules": true
        }
      ]
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": {
    "development": [
      "last 1 version"
    ],
    "production": [
      "> 1%",
      "last 2 versions",
      "not ie <= 10"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "plugins": [
      "babel",
      "compat",
      "html",
      "import",
      "prettier"
    ],
    "parserOptions": {
      "parser": "babel-parser",
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true
      }
    },
    "rules": {},
    "ignorePatterns": [
      "dist/",
      ".history/",
      ".parcel-cache/",
      "node_modules/"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "tabWidth": 2
  }
}
